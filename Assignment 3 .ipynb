   "source": [
    "# Write a program to check whether the last digit of a number (entered by user) is divisible by 3 or not "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fdff3067",
   "metadata": {},
   "outputs": [],
   "source": [
    "num = int(input(\" enter a number:\"))\n",
    "\n",
    "num1 = num%10\n",
    "\n",
    "if num1%3 ==0:\n",
    "    print()\n"
   ]
